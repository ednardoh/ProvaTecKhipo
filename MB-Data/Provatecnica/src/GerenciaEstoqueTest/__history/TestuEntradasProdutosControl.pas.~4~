    procedure TestGetId;
    procedure TestSalvar;
  end;

implementation

procedure TestTEntradasProdutosControl.SetUp;
begin
  FEntradasProdutosControl := TEntradasProdutosControl.Create;
end;

procedure TestTEntradasProdutosControl.TearDown;
begin
  FEntradasProdutosControl.Free;
  FEntradasProdutosControl := nil;
end;

procedure TestTEntradasProdutosControl.TestSalvar;
var
  ReturnValue: Boolean;
begin
  ReturnValue := FEntradasProdutosControl.Salvar;
end;

