begin

end;

function TEntradasProdutosModel.Salvar: Boolean;
begin

end;

procedure TEntradasProdutosModel.SetAcao(const Value: TAcao);
begin

end;

procedure TEntradasProdutosModel.SetCodigoBar(const Value: string);
begin

end;

procedure TEntradasProdutosModel.SetDescricao(const Value: string);
begin

end;

procedure TEntradasProdutosModel.SetId(const Value: Integer);
begin

end;

procedure TEntradasProdutosModel.SetPrecoCompra(const Value: Double);
begin

end;

procedure TEntradasProdutosModel.SetPrecoVenda(const Value: Double);
begin

end;

procedure TEntradasProdutosModel.SetQtdEstoque(const Value: Double);
begin

end;

end.
